app: GameMaker
-
settings(): key_hold = 32

play:
  key(f5)

debug:
  key(f6)

clean:
  key(ctrl-f7)

if:
  key(f4)
  sleep(100ms)
  key(2)

condition:
  key(f4)
  sleep(100ms)
  key(3)

for loop:
  key(f4)
  sleep(100ms)
  key(5)

switch:
  key(f4)
  sleep(100ms)
  key(9)

section:
  key(f4)
  sleep(100ms)
  key(r)

comment:
  key(f4)
  sleep(100ms)
  key(m)
